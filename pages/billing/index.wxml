<!--pages/billing/index.wxml-->
<view class="container">
    <view class="number">
        <text>当前单车编号：{{number}}</text>
    </view>
    <view class="time">
        <view class="time-title">
            <text>{{blling}}</text>
        </view>
        <view class="time-content">
        {{hours}}:{{minutes}}:{{second}}
        </view>
    </view>
    <view class="endride">
        <button type="warn" bindtap="endirde" disabled = "{{disabled}}">结束骑行</button>
        <button type="primary" bindtap="movetoIndex">回到首页</button>
    </view>
</view>
